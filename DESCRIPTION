Package: trelliscopecore
Title: Create TrelliscopeJS Specifications
Version: 0.0.0.9000
Authors@R: c(
      person("Ryan", "Hafen", email = "rhafen@gmail.com", role = c("aut", "cre")),
      person("Barret", "Schloerke", email = "schloerke@gmail.com", role = "aut")
